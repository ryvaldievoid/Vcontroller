<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/window_translucent"
    tools:context="voidream.vcontroller.Statistic">

    <LinearLayout
        android:layout_width="300dp"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:id="@+id/relativeLayout19"
        android:layout_weight="1"
        android:layout_marginTop="60dp"
        android:layout_gravity="center_horizontal"
        android:background="@android:color/white"
        android:layout_marginBottom="10dp">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="225dp"
            android:background="@color/text1">

            <com.jjoe64.graphview.GraphView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:id="@+id/graph" />
        </RelativeLayout>

        <ImageView
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:id="@+id/imageView10"
            android:layout_gravity="center_horizontal"
            android:src="@color/color_main_2"
            android:layout_marginTop="5dp" />

        <ListView
            android:scrollbarThumbVertical="@drawable/custom_scollbar"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/listview_statistic_output"
            android:divider="@android:color/transparent"
            android:background="@android:color/white" />
    </LinearLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="@android:color/black"></RelativeLayout>
</LinearLayout>
