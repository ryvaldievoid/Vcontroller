<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="290dp"
        android:layout_height="100dp"
        android:layout_gravity="center_horizontal"
        android:elevation="4dp">

        <RelativeLayout
            android:layout_width="60dp"
            android:layout_height="match_parent" >

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/dummy_number"
                android:id="@+id/textview_statistic_output_number"
                android:textColor="@color/text2"
                android:paddingRight="2dp"
                android:paddingLeft="2dp"
                android:layout_marginLeft="2dp"
                android:layout_marginTop="2dp"
                android:textSize="12dp" />

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/out_1"
                android:id="@+id/imageview_statistic_output_image"
                android:layout_centerInParent="true" />
        </RelativeLayout>

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical">

            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/dummy"
                    android:id="@+id/textview_statistic_output_name"
                    android:textColor="@color/text1"
                    android:textStyle="bold"
                    android:textSize="12dp" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/kurung_buka"
                    android:id="@+id/textView4"
                    android:textColor="@color/text1"
                    android:layout_below="@+id/textview_statistic_output_name"
                    android:layout_alignParentLeft="true"
                    android:layout_gravity="center_horizontal"
                    android:textSize="12dp"
                    android:layout_marginLeft="4dp" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/dummy"
                    android:id="@+id/textview_statistic_output_position"
                    android:textColor="@color/text1"
                    android:layout_below="@+id/textview_statistic_output_name"
                    android:layout_alignParentLeft="true"
                    android:layout_gravity="center_horizontal"
                    android:textSize="12dp" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/kurung_tutup"
                    android:id="@+id/textView6"
                    android:textColor="@color/text1"
                    android:layout_below="@+id/textview_statistic_output_name"
                    android:layout_alignParentLeft="true"
                    android:layout_gravity="center_horizontal"
                    android:textSize="12dp" />

            </LinearLayout>

            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_gravity="center_horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/power"
                    android:id="@+id/textView5"
                    android:textColor="@color/text2"
                    android:layout_toRightOf="@+id/textView26"
                    android:layout_toEndOf="@+id/textView26"
                    android:textSize="12dp"
                    android:layout_marginLeft="4dp" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/dummy"
                    android:id="@+id/textview_statistic_output_power"
                    android:textColor="@color/text1"
                    android:layout_toRightOf="@+id/textView26"
                    android:layout_toEndOf="@+id/textView26"
                    android:textSize="12dp"
                    android:layout_marginLeft="4dp" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/watt"
                    android:id="@+id/textView12"
                    android:textColor="@color/text2"
                    android:layout_toRightOf="@+id/textview_statistic_output_power"
                    android:layout_toEndOf="@+id/textview_statistic_output_power"
                    android:paddingLeft="2dp"
                    android:textSize="12dp" />
            </LinearLayout>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/usage"
                android:id="@+id/textView48"
                android:textColor="@color/text2"
                android:layout_toRightOf="@+id/textview_statistic_output_power"
                android:layout_toEndOf="@+id/textview_statistic_output_power"
                android:paddingLeft="2dp"
                android:textSize="12dp"
                android:layout_gravity="center_horizontal"
                android:layout_marginTop="5dp" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="120dp"
                    android:layout_height="wrap_content">

                    <LinearLayout
                        android:orientation="horizontal"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/linearLayout8"
                        android:layout_weight="1" >

                        <TextView
                            android:layout_width="54dp"
                            android:layout_height="wrap_content"
                            android:text="@string/day"
                            android:id="@+id/textView60"
                            android:textColor="@color/text2"
                            android:layout_gravity="center_vertical"
                            android:textSize="12dp" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/double_dot"
                            android:id="@+id/textView71"
                            android:paddingRight="4dp"
                            android:textColor="@color/text2"
                            android:layout_gravity="center_vertical"
                            android:textSize="12dp" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/dummy"
                            android:id="@+id/textview_statistic_day"
                            android:textColor="@color/color_main_2"
                            android:layout_gravity="center_vertical"
                            android:textSize="12dp" />
                    </LinearLayout>

                    <LinearLayout
                        android:orientation="horizontal"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1" >

                        <TextView
                            android:layout_width="54dp"
                            android:layout_height="wrap_content"
                            android:text="@string/average"
                            android:id="@+id/textView77"
                            android:textColor="@color/text2"
                            android:layout_gravity="center_vertical"
                            android:textSize="12dp" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/double_dot"
                            android:id="@+id/textView78"
                            android:paddingRight="4dp"
                            android:textColor="@color/text2"
                            android:layout_gravity="center_vertical"
                            android:textSize="12dp" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/dummy"
                            android:id="@+id/textview_statistic_average"
                            android:textColor="@color/color_main_2"
                            android:layout_gravity="center_vertical"
                            android:textSize="12dp" />
                    </LinearLayout>
                </LinearLayout>

                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <LinearLayout
                        android:orientation="horizontal"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/linearLayout9"
                        android:layout_weight="1" >

                        <TextView
                            android:layout_width="54dp"
                            android:layout_height="wrap_content"
                            android:text="@string/week"
                            android:id="@+id/textView80"
                            android:textColor="@color/text2"
                            android:layout_gravity="center_vertical"
                            android:textSize="12dp" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/double_dot"
                            android:id="@+id/textView81"
                            android:paddingRight="4dp"
                            android:textColor="@color/text2"
                            android:layout_gravity="center_vertical"
                            android:textSize="12dp" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/dummy"
                            android:id="@+id/textview_statistic_week"
                            android:textColor="@color/color_main_2"
                            android:layout_gravity="center_vertical"
                            android:textSize="12dp" />
                    </LinearLayout>

                    <LinearLayout
                        android:orientation="horizontal"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/linearLayout10"
                        android:layout_weight="1" >

                        <TextView
                            android:layout_width="54dp"
                            android:layout_height="wrap_content"
                            android:text="@string/month"
                            android:id="@+id/textView83"
                            android:textColor="@color/text2"
                            android:layout_gravity="center_vertical"
                            android:textSize="12dp" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/double_dot"
                            android:id="@+id/textView84"
                            android:paddingRight="4dp"
                            android:textColor="@color/text2"
                            android:layout_gravity="center_vertical"
                            android:textSize="12dp" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/dummy"
                            android:id="@+id/textview_statistic_month"
                            android:textColor="@color/color_main_2"
                            android:layout_gravity="center_vertical"
                            android:textSize="12dp" />
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>

    </LinearLayout>
</LinearLayout>